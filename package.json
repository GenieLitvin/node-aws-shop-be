{
  "name": "node-aws-shop-be",
  "version": "0.1.0",
  "bin": {
    "node-aws-shop-be": "bin/node-aws-shop-be.js"
  },
  "scripts": {
    "build": "node build.js",
    "deploy": "npm run build & cdk deploy",
