    "aws-cdk": "2.144.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.600.0",
    "@aws-sdk/util-dynamodb": "^3.600.0",
    "aws-cdk-lib": "2.144.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "esbuild": "^0.21.5",
    "source-map-support": "^0.5.21",
    "uuid": "^10.0.0"
  }
