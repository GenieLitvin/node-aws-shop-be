{
  "name": "import-service",
  "version": "1.0.0",
  "description": "This project is an AWS CDK-based application that deploys an API Gateway with Lambda functions for a product service. The service includes endpoints to retrieve a list of products and get product details by ID. It also includes Swagger documentation and unit tests using Jest.",
  "scripts": {
    "build": "node build.js",
    "deploy": "npm run build & cdk deploy",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --fix",
    "format": "prettier --write '**/*.ts'"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.140",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "aws-cdk": "2.144.0",
    "aws-sdk-client-mock": "^4.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/s3-request-presigner": "^3.600.0",
    "@smithy/util-stream": "^3.0.4",
    "aws-cdk-lib": "2.144.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "csv-parser": "^3.0.0",
    "esbuild": "^0.21.5",
    "source-map-support": "^0.5.21"
  }
}
